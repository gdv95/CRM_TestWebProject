

@model List<CRMUser>


@{
    ViewBag.Title = "List";
}

<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <title>Список сотрудников</title>
</head>
<body>
    <table border="1">
        <col style="width:40%">
        <col style="width:40%">
        <col style="width:40%">
        <caption>Список сотрудников</caption>
        <tr>
            <th>Имя</th>
            <th>Фамилия</th>
            <th>Логин</th>
        </tr>
        @{
            foreach (var i in Model)
            {
                <tr>
                    <td>@i.FirstName</td>
                    <td>@i.LastName</td>
                    <td>@i.Login</td>
                </tr>
            }
        }

    </table>
</body>
</html>


